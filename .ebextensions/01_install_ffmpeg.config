commands:
  01_install_epel:
    command: "sudo dnf install -y https://dl.fedoraproject.org/pub/epel/epel-release-latest-8.noarch.rpm"
  02_enable_epel:
    command: "sudo dnf config-manager --set-enabled epel"
  03_install_rpmfusion:
    command: "sudo dnf install -y https://download1.rpmfusion.org/free/el/rpmfusion-free-release-8.noarch.rpm"
  04_install_ffmpeg:
    command: "sudo dnf install -y ffmpeg"